{% extends 'base.html.twig' %}

 {% block pageheader %} 
    <h1>CommentNote</h1>
 {% endblock %} 

{% block body %}

    <div class="btn-toolbar">
    <div class="btn-group pull-right">
        <a href="{{ path('admin_comment_note_index') }}" class="btn btn-default">
            <span class="glyphicon glyphicon-arrow-left"></span> Back
        </a>
        <a href="{{ path('admin_comment_note_edit', { 'id': commentNote.id }) }}" class="btn btn-default">
            <span class='glyphicon glyphicon-edit'></span> Edit
        </a>
        <a href="{{ path('admin_comment_note_delete', { 'id': commentNote.id }) }}" class="btn btn-default" data-confirm="Are you sure you want to delete this CommentNote?">
            <span class='glyphicon glyphicon-remove'></span> Delete
        </a>
    </div>
</div>
    <table class="table table-bordered table-condensed table-hover table-striped">
        <tbody>
            <tr>
                <th>Content</th>
            <td>{{ commentNote.content }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if commentNote.created %}{{ commentNote.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if commentNote.updated %}{{ commentNote.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

{% endblock %}
