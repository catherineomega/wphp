{% extends 'base.html.twig' %}

{% block pageheader %} 
    <h1>Geonames</h1>
{% endblock %} 

{% block body %}

    <div class="btn-toolbar">
        <div class="btn-group pull-right">
            <a href="javascript: history.go(-1)" class="btn btn-default">
                <span class="glyphicon glyphicon-arrow-left"></span> Back
            </a>
        </div>
    </div>
    <table class="table table-bordered table-condensed table-hover table-striped">
        <tbody>
            <tr>
                <th>Geoname ID</th>
                <td>{{ geoname.geonameid }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ geoname.name }}</td>
            </tr>
            <tr>
                <th>ASCII name</th>
                <td>{{ geoname.asciiname }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ geoname.latitude }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ geoname.longitude }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ geoname.country }}</td>
            </tr>
            <tr>
                <th>Admin 1</th>
                <td>{{ geoname.admin1 }}</td>
            </tr>
            <tr>
                <th>Admin 2</th>
                <td>{{ geoname.admin2 }}</td>
            </tr>
            <tr>
                <th>Admin 3</th>
                <td>{{ geoname.admin3 }}</td>
            </tr>
            <tr>
                <th>Admin 4</th>
                <td>{{ geoname.admin4 }}</td>
            </tr>
        </tbody>
    </table>
            
    {% embed 'AppBundle:Title:partials/page.html.twig' with {'titles': titles} %}
    {% endembed %}

{% endblock %}
