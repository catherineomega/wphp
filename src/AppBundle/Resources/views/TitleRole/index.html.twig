{% extends 'base.html.twig' %}

 {% block pageheader %} 
    <h1>TitleRole List</h1>
    <p class="count">
        Displaying {{ titleRoles|length }} titleRoles of {{ titleRoles.getTotalItemCount }} total.
    </p>
 {% endblock %} 

{% block body %}

        <div class='btn-toolbar pull-right'>
        <div class='btn-group'>
            <a href="{{ path('titlerole_new') }}" class="btn btn-default">
                <span class="glyphicon glyphicon-plus"></span> New
            </a>
            <a href="{{ path('titlerole_search') }}" class="btn btn-default">
                <span class="glyphicon glyphicon-search"></span> Search
            </a>
        </div>
    </div>
        
    <table class="table table-bordered table-condensed table-hover table-striped">
        <thead>
            <tr>            </tr>
        </thead>
        <tbody>
        {% for titleRole in titleRoles %}
            <tr>
            </tr>
        {% endfor %}
        </tbody>
    </table>
        
    <div class="navigation">
        {{ knp_pagination_render(titleRoles) }}
    </div>

{% endblock %}
